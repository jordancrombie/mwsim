{
  "expo": {
    "name": "mwsim",
    "slug": "mwsim",
    "scheme": "mwsim",
    "version": "1.7.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#E3F2FD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.banksim.wsim",
      "buildNumber": "2",
      "associatedDomains": [
        "applinks:wsim.banksim.ca",
        "applinks:wsim-dev.banksim.ca",
        "applinks:transfer.banksim.ca",
        "applinks:transfersim-dev.banksim.ca"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "mwsim needs camera access to scan payment QR codes and take profile photos",
        "NSPhotoLibraryUsageDescription": "mwsim needs access to your photo library to select a profile photo",
        "LSApplicationQueriesSchemes": [
          "googlechrome",
          "googlechromes",
          "firefox",
          "microsoft-edge",
          "opera",
          "brave"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.banksim.wsim",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-image-picker",
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#1976D2",
          "sounds": []
        }
      ],
      "./plugins/withSettingsBundle",
      "./plugins/withSettingsDefaults",
      "./plugins/withSigningTeam",
      "./plugins/withXcodeOptimizations"
    ]
  }
}
