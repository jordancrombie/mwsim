{
  "expo": {
    "name": "mwsim",
    "slug": "mwsim",
    "scheme": "mwsim",
    "version": "1.2.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#E3F2FD"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.banksim.wsim",
      "buildNumber": "13",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "mwsim needs camera access to scan payment QR codes",
        "LSApplicationQueriesSchemes": [
          "googlechrome",
          "googlechromes",
          "firefox",
          "microsoft-edge",
          "opera",
          "brave"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.banksim.wsim",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "./plugins/withSettingsBundle",
      "./plugins/withSettingsDefaults",
      "./plugins/withSigningTeam"
    ]
  }
}
